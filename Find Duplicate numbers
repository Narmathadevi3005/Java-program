import java.util.*;
import java.util.LinkedHashSet;
public class Main
{
public static void main(String[] args)
{
Scanner obj=new Scanner(System.in);
int n=obj.nextInt();
int[] arr=new int[n];
for(int i=0;i<n;i++)
{
set.add(arr[i];
}
System.out.print(set.size() +" ");
for(Integer i:set)
{
System.out.print(i+ " ");
}
}
}
