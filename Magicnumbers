import java.util.*;
class magicnumber
{
  public static void main(String[] args)
  {
    Scanner in=new Scanner(System.in);
    int num=in.nextInt();
    int n=num;
    while(n>0)
    {
      int d=n%10;
      n=n/10;
      sum=sum+d;
     }
    n=sum;
 }
if(n==1)
{
  System.out.println("YES");
}
else
{
  System.out.println("NO");
}
}
